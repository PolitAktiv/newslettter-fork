<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN"
"http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">

<routes>
    <route>
        <pattern>/subscribe</pattern>
        <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
        <implicit-parameter name="javax.portlet.action">confirmSubscription</implicit-parameter>
    </route>
    <route>
        <pattern>/unsubscribe</pattern>
        <implicit-parameter name="view">unsubscriptionAsk</implicit-parameter>
    </route>
</routes>